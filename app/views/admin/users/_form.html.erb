<%= semantic_form_for [:admin, @user] do |f| %>
   <%= f.inputs :name => 'Create User' do  %>
       <%= f.input :name  %>
       <%= f.input :email  %>
       <%= f.input :password  %>
       <%= f.input :password_confirmation  %>
       <%= f.input :address  %>
       <%= f.input :phone  %>
       <%= f.input :role  %>
       <%#= f.input :store_id, :as => :select  %>

        <%= f.actions do %>
            <%= f.action :submit, :as => :button %>
            <%= f.action :cancel, :as => :link %>
        <% end %>
    <% end %>
<% end  %>